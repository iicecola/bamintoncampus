{"ast":null,"code":"import _Message2 from \"element-ui/lib/theme-chalk/message.css\";\nimport \"element-ui/lib/theme-chalk/base.css\";\nimport _Message from \"element-ui/lib/message\";\nimport \"core-js/modules/es.array.push.js\";\n//导入信息提示插件\n\nexport default {\n  name: 'Register',\n  data() {\n    return {\n      userAccount: '',\n      userPassword: '',\n      userRepassword: '',\n      userNickname: '',\n      userAgreement: false\n    };\n  },\n  methods: {\n    register() {\n      if (this.userAccount === '' || this.userPassword === '') {\n        return _Message({\n          message: '账号或密码不能为空',\n          type: 'warning',\n          showClose: true\n        });\n      } else if (this.userPassword !== this.userRepassword) {\n        return this.$message.error('两次密码输入不一致');\n      } else if (this.userNickname === '') {\n        return this.$message({\n          message: '昵称不能为空',\n          type: 'warning'\n        });\n      } else if (!this.userAgreement) {\n        return this.$message({\n          message: '请阅读并同意用户协议',\n          type: 'warning'\n        });\n      } else {\n        this.$message({\n          message: '注册成功',\n          type: 'success'\n        });\n\n        //保存用户信息到本地\n        localStorage.setItem('userAccount', this.userAccount);\n        localStorage.setItem('userPassword', this.userPassword);\n        localStorage.setItem('userNickname', this.userNickname);\n\n        //通过vuex传递用户信息\n        this.$store.commit('userInfo/LOGIN', [this.userAccount, this.userPassword, this.userNickname]);\n        this.$router.push('/login');\n      }\n    }\n  }\n};","map":{"version":3,"names":["name","data","userAccount","userPassword","userRepassword","userNickname","userAgreement","methods","register","_Message","message","type","showClose","$message","error","localStorage","setItem","$store","commit","$router","push"],"sources":["src/views/Register.vue"],"sourcesContent":["<template>\r\n  <div class=\"register\">\r\n    <div class=\"register-cover\">\r\n      <div class=\"register-form\">\r\n        <form action=\"#home\">\r\n          <h2 style=\"text-align:center;font-size: 30px;color:skyblue\">注&nbsp;&nbsp;&nbsp;册</h2>\r\n          <hr>\r\n          <div class=\"register-input\">\r\n            <label style=\"margin-left:30px\" for=\"useraccount\">账号:</label>\r\n            <el-input \r\n              id=\"useraccount\"\r\n              v-model.trim=\"userAccount\" \r\n              style=\"width:300px;margin-left:50px;margin-bottom:30px;\" \r\n              size=\"medium \" \r\n              type=\"text\" \r\n              placeholder=\"请输入账号\">\r\n            </el-input><br>\r\n\r\n            <label style=\"margin-left:30px\" for=\"password\">密码:</label>\r\n            <el-input \r\n              id=\"password\"\r\n              v-model=\"userPassword\" \r\n              style=\"width:300px;margin-left:50px;margin-bottom:30px;\" \r\n              size=\"medium \" \r\n              type=\"text\" \r\n              placeholder=\"请输入密码\">\r\n            </el-input><br>\r\n\r\n            <label style=\"margin-left:30px\" for=\"repassword\">确认密码:</label>\r\n            <el-input \r\n              id=\"repassword\"\r\n              v-model=\"userRepassword\" \r\n              style=\"width:300px;margin-left:20px;margin-bottom:30px;\" \r\n              size=\"medium \" \r\n              type=\"text\" \r\n              placeholder=\"再次输入密码\">\r\n            </el-input><br>\r\n\r\n            <label style=\"margin-left:30px\" for=\"nickname\">昵称:</label>\r\n            <el-input \r\n              id=\"nickname\"\r\n              v-model.trim=\"userNickname\" \r\n              style=\"width:300px;margin-left:50px;margin-bottom:30px;\" \r\n              size=\"medium \" \r\n              type=\"text\" \r\n              placeholder=\"请输入昵称\">\r\n            </el-input><br>\r\n\r\n            <el-checkbox v-model=\"userAgreement\">我已阅读并同意《用户协议》</el-checkbox><br>\r\n            <el-button\r\n              @click=\"register\"\r\n              style=\"width:100px;height: 40px;margin-top: 20px\" \r\n              type=\"primary\" plain>\r\n            注册</el-button>\r\n          </div>\r\n        </form>\r\n      </div>\r\n    </div>\r\n  </div>\r\n  \r\n</template>\r\n\r\n<script>\r\n//导入信息提示插件\r\nimport { Message } from 'element-ui'\r\n\r\nexport default {\r\n    name:'Register',\r\n    data(){\r\n        return {\r\n          userAccount: '',\r\n          userPassword:'',\r\n          userRepassword:'',\r\n          userNickname:'',\r\n          userAgreement:false\r\n        }\r\n    },\r\n    methods:{\r\n      register(){\r\n        if(this.userAccount === '' || this.userPassword === ''){\r\n          return Message({\r\n            message:'账号或密码不能为空',\r\n            type:'warning',\r\n            showClose:true\r\n          })\r\n        }else if(this.userPassword !== this.userRepassword){\r\n          return this.$message.error('两次密码输入不一致')\r\n        }else if(this.userNickname === ''){\r\n          return this.$message({\r\n            message:'昵称不能为空',\r\n            type:'warning'\r\n          })\r\n        }else if(!this.userAgreement){\r\n          return this.$message({\r\n            message:'请阅读并同意用户协议',\r\n            type:'warning'\r\n          })\r\n        }else{\r\n          this.$message({\r\n            message:'注册成功',\r\n            type:'success'\r\n          })\r\n\r\n          //保存用户信息到本地\r\n          localStorage.setItem('userAccount',this.userAccount)\r\n          localStorage.setItem('userPassword',this.userPassword)\r\n          localStorage.setItem('userNickname',this.userNickname)\r\n\r\n          //通过vuex传递用户信息\r\n          this.$store.commit('userInfo/LOGIN',[this.userAccount,this.userPassword,this.userNickname])\r\n          this.$router.push('/login')\r\n        }\r\n        \r\n      }\r\n    }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n  * {\r\n    margin: 0;\r\n    padding: 0;\r\n    box-sizing: border-box;\r\n  }\r\n\r\n  .register {\r\n
    },0)\r\n      },\r\n\r\n      // 鼠标移入轮播图停止轮播图自动播放\r\n      stopCarouselInterval(){\r\n        if(this.carouselInterval){\r\n          clearInterval(this.carouselInterval)\r\n          this.carouselInterval = null\r\n        }\r\n\r\n        // 鼠标移入轮播图时显示轮播图左右按钮\r\n        this.carouselBtnShow = true\r\n      },\r\n\r\n      // 鼠标移出轮播图重新开始轮播图自动播放\r\n      startCarouselInterval(){\r\n        if(!this.carouselInterval){\r\n          this.carouselInterval = setInterval(() => {\r\n            this.carouselTurnRight()\r\n          }, 5000)\r\n        }\r\n\r\n        // 鼠标移入轮播图时隐藏轮播图左右按钮\r\n        this.carouselBtnShow = false\r\n      },\r\n\r\n      // 轮播图小圆点点击事件\r\n      carouselDotClick(index){\r\n        this.carouselIndex = index\r\n        const carouselDiv = document.querySelector('.carousel-img')\r\n        carouselDiv.style.transform = this.carouselTransform\r\n      }\r\n    },\r\n\r\n    watch: {\r\n    },\r\n\r\n    mounted(){\r\n      // 轮播图自动播放\r\n      this.carouselInterval = setInterval(() => {\r\n        this.carouselTurnRight()\r\n      }, 5000)\r\n    },\r\n\r\n    beforeDestroy(){\r\n      // 清除轮播图自动播放定时器\r\n      console.log('beforeDestroy')\r\n      clearInterval(this.carouselInterval)\r\n    }\r\n}\r\n</script>\r\n\r\n\r\n\r\n<style scoped>\r\n\r\n  * {\r\n    margin: 0;\r\n    padding: 0;\r\n    box-sizing: border-box;\r\n  }\r\n\r\n  .home-container {\r\n    position: relative;\r\n    width: 100%;\r\n    height: 1000px;\r\n    clear: both;\r\n    background-color: #f7f7f7;\r\n  }\r\n\r\n  .home-main {\r\n    position: absolute;\r\n    top:0;\r\n    left:0;\r\n  }\r\n\r\n  ul {\r\n    list-style: none;\r\n  }\r\n\r\n  a {\r\n    text-decoration: none;\r\n    color: inherit;\r\n  }\r\n\r\n  /* 左部边栏 */\r\n  .left-aside {\r\n    position: absolute;\r\n    top: 0;\r\n    left: 150px;\r\n    width: 230px;\r\n    border: 1px solid #eee;\r\n    border-bottom: none;\r\n    border-right: none;\r\n  }\r\n\r\n  .left-aside-item {\r\n    display: block;\r\n    width: 100%;\r\n    height: 110px;\r\n    padding: 29px;\r\n    border-bottom: 1px solid #eee;\r\n    border-right: 1px solid #eee;\r\n    color: rgb(49, 49, 49);\r\n    background-color: #fff;\r\n    cursor: pointer;\r\n  }\r\n\r\n  .left-aside-item-subtitle {\r\n    margin-top: 10px;\r\n    font-size: 14px;\r\n    color: #838383\r\n  }\r\n\r\n  .left-aside-font-acitve:hover {\r\n    border-right: none;\r\n    color: skyblue;\r\n  }\r\n\r\n  .left-aside-item-content {\r\n    position: absolute;\r\n    top:0;\r\n    left: 229.5px;\r\n    width: 230px;\r\n    height: 440px;\r\n    border: 1px solid #eee;\r\n    border-left: none;\r\n    background-color: #fff;\r\n    z-index: 999;\r\n  }\r\n\r\n  /* 中间轮播图 */\r\n  .carousel-container {\r\n    position: absolute;\r\n    top: 10px;\r\n    left: 400px;\r\n    width: 690px;\r\n    height: 430px;\r\n    border-radius: 5px;\r\n    overflow: hidden;\r\n    background-color: #fff;\r\n    cursor: pointer;\r\n  }\r\n\r\n  .carousel-img {\r\n    display: flex;\r\n    flex-wrap: nowrap;\r\n  }\r\n\r\n\r\n  .carousel-btn {\r\n    width: 45px;\r\n    height: 50px;\r\n    line-height: 50px;\r\n    text-align: center;\r\n    font-size: 20px;\r\n    color: #b8b8b8;\r\n    background-color: rgba(0,0,0,0.5);\r\n    z-index: 999;\r\n  }\r\n\r\n\r\n  .carousel-btn:hover {\r\n    color: rgb(218, 218, 218);\r\n    background-color: rgba(0, 179, 255, 0.663);\r\n  }\r\n\r\n  .carousel-btn:active {\r\n    color: rgb(218, 218, 218);\r\n    background-color: rgba(0, 97, 138, 0.663);\r\n  }\r\n\r\n  .carousel-btn-left {\r\n    position: absolute;\r\n    top: 50%;\r\n    left: 0;\r\n    border-radius: 0 15px 15px 0;\r\n    transform: translate(0,-50%);\r\n  }\r\n\r\n  .carousel-btn-right {\r\n    position: absolute;\r\n    top: 50%;\r\n    left: 100%;\r\n    border-radius: 15px 0 0 15px;\r\n    transform: translate(-100%,-50%);\r\n  }\r\n\r\n  /* 轮播图按钮的出现和消失，使用vue的过度实现 */\r\n  /* 左按钮 */\r\n  .left-btn-enter , .left-btn-leave-to {\r\n    opac